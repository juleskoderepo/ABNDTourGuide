    <string name="fun_zilker">Zilker Metropolitan Park</string>
    <string name="fun_zilker_cat">Recreation/Picnic</string>
    <string name="fun_zilker_addr">2100 Barton Springs Rd, Austin, TX, 78704</string>
    <string name="fun_zilker_web">http://austintexas.gov/department/zilker-metropolitan-park</string>
    <string name="fun_zilker_tip">Play some disc golf or sand volleyball. Catch a show at the
        Hillside Theatre. Blues on the Green in the summer. ACL Music Festival in late summer.</string>
    <string name="fun_lb_lake">Lady Bird Lake</string>
    <string name="fun_lb_lake_cat">Recreation/Picnic</string>
    <string name="fun_lb_lake_addr">Downtown</string>
    <string name="fun_lb_lake_web">http://www.austintexas.gov/page/lady-bird-lake</string>
    <string name="fun_lb_lake_tip">Look for parking off S 1st and Riverside or N Lamar and
        W Cesar Chavez. Go for a stroll, run, or bike along the 10-mile Butler
        Hike-and-Bike Trail. Take a pic with the Stevie Ray Vaughn statue.</string>
    <string name="fun_butler_park">Butler Park Pitch and Putt</string>
    <string name="fun_butler_park_cat">Golf</string>
    <string name="fun_butler_park_addr">201 Lee Barton Dr, Austin, TX 78704</string>
    <string name="fun_butler_park_web">http://www.butlerparkpitchandputt.com/</string>
