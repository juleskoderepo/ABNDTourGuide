    <string name="fun_alamo_dh">Alamo Drafthouse</string>
    <string name="fun_alamo_dh_cat">Movies</string>
    <string name="fun_alamo_dh_addr">320 E 6th St, Austin, TX 78701</string>
    <string name="fun_alamo_dh_web">https://drafthouse.com/</string>
    <string name="fun_alamo_dh_tip">Multiple locations. Get your meal and a show all in one place.
