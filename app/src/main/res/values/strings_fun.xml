    <string name="fun_mt_bonnell">Mount Bonnell</string>
    <string name="fun_mt_bonnell_cat">Hiking/Picnic</string>
    <string name="fun_mt_bonnell_addr">3800 Mount Bonnell Rd, Austin, TX 78731</string>
    <string name="fun_mt_bonnell_web">http://www.austintexas.gov/department/mount-bonnell</string>
